-			return ONE_HOUR;
-		} else if(timeDown < THREE_DAYS) {
-			return FOUR_HOURS;
-		} else if(timeDown < SEVEN_DAYS) {
-			return EIGHTEEN_HOURS;
-		} else {
-			return THIRTYSIX_HOURS;
-		}
-	}
-}
