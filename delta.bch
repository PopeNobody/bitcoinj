-		if(size <= 10) {
-			partitions.add(createPartitionList(descriptors, 0, size));
-			return partitions;
-		} else if(size <= (MIN_DL_REQUESTS * MAX_DL_PER_REQUEST)) {
-			final int chunk = size / MIN_DL_REQUESTS;
