+
+    private byte[] bitwiseScript(byte[] a, byte[] b, String opcode) {
+        ScriptBuilder builder = new ScriptBuilder();
+        if (a != null) {
+            builder.data(a);
+        }
+        if (b != null) {
+            builder.data(b);
+        }
+        builder.op(ScriptOpCodes.getOpCode(opcode));
+        return executeMonolithScript(builder.build());
+    }
