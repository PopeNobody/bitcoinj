-					sz++;
-					over--;
-				}
-				partitions.add(createPartitionList(descriptors, off, sz));
-				off += sz;
-			}
-			return partitions;
-			
-		} else {
-			int off = 0;
-			while(off < descriptors.size()) {
-				partitions.add(createPartitionList(descriptors, off, MAX_DL_PER_REQUEST));
-				off += MAX_DL_PER_REQUEST;
-			}
-			return partitions;	
-		}
-	}
