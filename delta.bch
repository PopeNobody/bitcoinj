-				if(loadingEventCode > 0) {
-					initializationTracker.notifyEvent(loadingEventCode);
-				}
-				return stream;
-			} catch (StreamConnectFailedException e) {
-				circuit.markForClose();
-				failCount += 1;
-			} catch (TimeoutException e) {
-				circuit.markForClose();
-			}
