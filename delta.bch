-		final StringBuilder sb = new StringBuilder();
-		while(buffer.hasRemaining()) {
-			char c = (char) (buffer.get() & 0xFF);
-			if(c == '\n') {
-				return new Line(sb.toString());
-			} else if(c != '\r') {
-				sb.append(c);
-			}
-		}
-		return new Line(sb.toString());
-	}
-}
